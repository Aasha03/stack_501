<!DOCTYPE html>
<!-- Created By CodingNepal - www.codingnepalweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>ALL COURSES</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/AllEvents.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"/>
    <script src="https://kit.fontawesome.com/9d595d1bf3.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <nav>
      <label for="check" class="checkbtn">
        <i class="fas fa-bars"></i>
      </label>
      <div>
      <label class="logo">Sports club</label>
    </div>
      <ul>
        <li class="link"><a class="active anc" href="dashBoard">Home</a></li>
        <li class="link"><a class="anc" href="AllEvents">All Events</a></li>
        <li class="link"><a class="anc" href="AddEvents">Add Event</a></li>
        <% if(flag === 0) { %>
        <li class="link"><a class="anc" href="myEvents">My Events</a></li>
        <% } %>
        <li class="link"><a class="anc"  href="#">About</a></li>
        <li class="profile">
          <i class="fa-regular fa-user"></i>
          <p class="proLink"><%= name %></p></li>
      </ul>
    </nav>

    <section class="event-section">
        <h1>Registers</h1>
        <ul class="event-list">
          <% formattedData.forEach(user => { %>
        <li class="event-item">
                <h2><%= user.firstName %></h2>
                <p><%= user.email %></p>

            </li>
          <% }); %>
        </ul>
      </section>


  </body>
</html>